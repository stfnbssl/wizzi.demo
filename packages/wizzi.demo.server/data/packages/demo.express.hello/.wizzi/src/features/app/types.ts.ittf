module
    import
        @ Application
        @ Router
        from 'express'
    import
        @ ConfigType
        from '../config'
    $if wzCtx.Build.useFilesystem
        import
            @ fsTypes
            from '../filesystem'
    export
        :type ModelBuilderType
            :{
                :p buildModel
                    :=>
                        :void
                        param options
                            :any
                            :optional
                :p applyExtraSetup
                    :=>
                        :void
                        param options
                            :any
                            :optional
    export
        :type ApiType
            :{
                :p name
                    :string
                :p initialize
                    :=>
                        :void
                        param initValues
                            :ref AppInitializerType
    export
        :type ControllerType
            :{
                :p path
                    :string
                :p router
                    :ref Router
                :p initialize
                    :=>
                        :void
                        param initValues
                            :ref AppInitializerType
    export
        :type MiddlewareType
            :=>
                :void
                param app
                    :ref Application
    export
        :type AppInitializerType
            :{
                :p config
                    :ref ConfigType
                :p globalApi
                    :any
                :p apis
                    :[
                        :ref ApiType
                :p controllers
                    :[
                        :ref ControllerType
                :p middlewaresPre
                    :[
                        :ref MiddlewareType
                :p middlewaresPost
                    :[
                        :ref MiddlewareType
                $if wzCtx.Build.useFilesystem
                    :p fsDb
                        :ref fsTypes.FsDb
