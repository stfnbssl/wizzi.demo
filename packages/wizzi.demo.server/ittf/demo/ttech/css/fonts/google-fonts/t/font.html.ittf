$group
    $params name, fsize

    h3 ${name} 100 font-size: ${fsize}
    . ${name}-100 font-size-${fsize}
        lorem 100
    h3 ${name} 300 font-size: ${fsize}
    . ${name}-300 font-size-${fsize}
        lorem 100
    h3 ${name} 900 font-size: ${fsize}
    . ${name}-900 font-size-${fsize} 
        lorem 20
