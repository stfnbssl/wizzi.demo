$group
    function updateResult
        var flex_container_code = 
			\b '.flex-container {\n' + 
			\b '\tdisplay: -webkit-flex;\n' + 
			\b '\tdisplay: flex;\n' + 
			\b flex_direction + 
            \b flex_wrap + 
			\b justify_content +
            \b align_items +
            \b align_content +
			\b '}\n'
        var flex_item_code = '.flex-item {\n' + flex + '}'
        var style = flex_container_code + '\n' + flex_item_code
        _ $("#demo_styles")
            ._ html(style)
